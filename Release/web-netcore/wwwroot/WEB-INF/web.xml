<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>BookMarksSync</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>uploadServlet</servlet-name>
		<servlet-class>com.UploadServlet</servlet-class>
		<init-param>
			<param-name>filepath</param-name>
			<param-value>uploadFile</param-value>
		</init-param>
		<init-param>
			<param-name>temppath</param-name>
			<param-value>temp</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>uploadServlet</servlet-name>
		<url-pattern>/uploadServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetIconServlet</servlet-name>
		<servlet-class>com.GetIconServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetIconServlet</servlet-name>
		<url-pattern>/GetIconServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UploadDBObj</servlet-name>
		<servlet-class>com.UploadDBObj</servlet-class>
		<init-param>
			<param-name>filepath</param-name>
			<param-value>uploadFile</param-value>
		</init-param>
		<init-param>
			<param-name>temppath</param-name>
			<param-value>temp</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadDBObj</servlet-name>
		<url-pattern>/UploadDBObj</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DownloadDBObj</servlet-name>
		<servlet-class>com.DownloadDBObj</servlet-class>
		<init-param>
			<param-name>filepath</param-name>
			<param-value>uploadFile</param-value>
		</init-param>
		<init-param>
			<param-name>temppath</param-name>
			<param-value>temp</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>DownloadDBObj</servlet-name>
		<url-pattern>/DownloadDBObj</url-pattern>
	</servlet-mapping>
</web-app>
